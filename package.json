{
  "name": "react-google-calendar-api",
  "version": "2.3.0",
  "description": "An API to manage your Google Calendar",
  "main": "./dist/ApiCalendar.js",
  "types": "./dist/ApiCalendar.d.ts",
  "scripts": {
    "build": "eslint . --ext .ts,.tsx && tsup src/ApiCalendar.ts --dts",
    "dev": "tsup src/ApiCalendar.ts --dts --watch",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Kubessandra/react-google-calendar-api.git"
  },
  "keywords": [
    "react",
    "google",
    "calendar",
    "api"
  ],
  "author": "Aymeric Beudaert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kubessandra/react-google-calendar-api/issues"
  },
  "homepage": "https://github.com/Kubessandra/react-google-calendar-api#readme",
  "devDependencies": {
    "@types/gapi": "^0.0.44",
    "@types/gapi.calendar": "^3.0.6",
    "@types/google.accounts": "^0.0.6",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "eslint": "^8.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.0",
    "jest": "^29.4.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.7.2",
    "tsup": "^6.0.1"
  },
  "files": [
    "dist"
  ]
}
